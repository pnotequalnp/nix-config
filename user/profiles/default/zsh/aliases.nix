{
  ga      = "git add";
  gc      = "git commit -m";
  gs      = "git status";
  tns     = "tmux new -s";
  tls     = "tmux ls";
  tat     = "tmux attach -t";
  ls      = "exa -lFT --group-directories-first --color=always --git --git-ignore --icons --level 1";
  lsa     = "exa -laTF --group-directories-first --color=always --git --icons --level 1";
  lst     = "exa -lTF --group-directories-first --color=always --git --git-ignore --icons";
  lsta    = "exa -laTF --group-directories-first --color=always --git --icons";
  ls2     = "exa -lTF --group-directories-first --color=always --git --git-ignore --icons --level 2";
  ls3     = "exa -lTF --group-directories-first --color=always --git --git-ignore --icons --level 3";
  ls4     = "exa -lTF --group-directories-first --color=always --git --git-ignore --icons --level 4";
  ls5     = "exa -lTF --group-directories-first --color=always --git --git-ignore --icons --level 5";
  ls2a    = "exa -laTF --group-directories-first --color=always --git --icons --level 2";
  ls3a    = "exa -laTF --group-directories-first --color=always --git --icons --level 3";
  ls4a    = "exa -laTF --group-directories-first --color=always --git --icons --level 4";
  ls5a    = "exa -laTF --group-directories-first --color=always --git --icons --level 5";
  lsg     = "exa -laTF --group-directories-first --color=always --icons --level 1";
  lstg    = "exa -laFT --group-directories-first --color=always --icons";
  ds      = "dirs -v | head -10";
}
