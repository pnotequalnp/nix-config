clear lock
clear control
clear mod1
clear mod4
clear mod5
keycode  37 = Alt_L Meta_L Alt_L Meta_L
keycode  64 = Control_L NoSymbol Control_L
keycode 108 = Super_L NoSymbol Super_L
keycode 107 = Alt_L Meta_L Alt_L Meta_L
keycode 105 = ISO_Level3_Shift NoSymbol ISO_Level3_Shift
keycode  66 = Escape Escape Escape
